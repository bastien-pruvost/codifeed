    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.1",
    "jsdom": "^26.1.0",
    "openapi-typescript": "^7.8.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.1",
    "web-vitals": "^5.0.2"
