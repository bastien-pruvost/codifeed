{
  "name": "@codifeed/web",
  "version": "1.0.0-alpha.1",
  "description": "A social network for developers, built as a full-stack project featuring a Flask REST API and a TypeScript-based React frontend.",
  "keywords": [],
  "author": "Bastien Pruvost",
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "scripts": {
    "start": "vite --port 3000",
    "build": "vite build && tsc",
