{
  "name": "@codifeed/web",
  "version": "1.0.0-alpha.1",
  "description": "A social network for developers, built as a full-stack project featuring a Flask REST API and a TypeScript-based React frontend.",
  "keywords": [],
  "author": "Bastien Pruvost",
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "scripts": {
    "dev": "pnpm run openapi-ts && vite --port 3000",
    "start": "vite --port 3000",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "format": "prettier --write .",
    "lint": "eslint --fix",
    "typecheck": "tsc",
    "check": "prettier --write . && eslint --fix && tsc",
    "openapi-ts": "openapi-typescript http://localhost:8000/openapi/openapi.json -o ./src/types/api.gen.ts --root-types --root-types-no-schema-prefix --make-paths-enum --generate-path-params"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@tanstack/react-router": "^1.129.8",
    "@tanstack/router-plugin": "^1.129.8",
    "@tanstack/zod-adapter": "^1.129.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "openapi-fetch": "^0.14.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.8",
    "tw-animate-css": "^1.3.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@tanstack/react-router-devtools": "^1.129.8",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "openapi-typescript": "^7.8.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "web-vitals": "^5.0.3"
  }
}
