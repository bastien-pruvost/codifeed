{
  "name": "codifeed-web",
  "version": "0.1.0",
  "description": "Codifeed frontend web app",
  "keywords": [],
  "author": "Bastien Limbour <bastien.limbour.dev@gmail.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22",
    "pnpm": "10"
  },
  "packageManager": "pnpm@10.18.1",
  "scripts": {
    "dev": "vite --port 3000 --host",
    "start": "vite --port 3000",
    "build": "vite build && tsc",
    "preview": "vite preview",
    "test": "vitest run",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc",
    "check": "pnpm run format:check && pnpm run lint:check && pnpm run typecheck",
    "fix": "pnpm run format:fix && pnpm run lint:fix",
    "openapi-ts": "openapi-typescript http://localhost:8000/openapi/openapi.json -o ./src/types/generated/api.gen.ts --root-types --root-types-no-schema-prefix --generate-path-params",
    "openapi-ts:watch": "chokidar '../api/**/*.py' -c 'pnpm run openapi-ts' --initial"
  },
  "dependencies": {
    "@mantine/hooks": "^8.3.4",
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/react-form": "^1.23.7",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-router": "^1.132.47",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.545.0",
    "openapi-fetch": "^0.14.1",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "vercel": "^48.10.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@tanstack/eslint-plugin-query": "^5.91.0",
    "@tanstack/eslint-plugin-router": "^1.132.31",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router-devtools": "^1.132.51",
    "@tanstack/router-plugin": "^1.132.51",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "22.18.10",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "openapi-typescript": "^7.9.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0"
  }
}
