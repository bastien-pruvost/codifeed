{
  "name": "codifeed-web",
  "version": "0.1.0",
  "description": "Codifeed frontend web app",
  "author": "Bastien Limbour",
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22",
    "pnpm": "10"
  },
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c",
  "scripts": {
    "dev": "vite --port 3000 --host",
    "start": "vite --port 3000",
    "build": "vite build && tsc",
    "preview": "vite preview",
    "test": "vitest run",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc",
    "check": "pnpm run format:check && pnpm run lint:check && pnpm run typecheck",
    "openapi-json": "cd ../ && make openapi-json",
    "openapi-ts": "openapi-typescript ../api/openapi/openapi.json -o ./src/types/generated/api.gen.ts --root-types --root-types-no-schema-prefix --generate-path-params",
    "openapi-ts:watch": "chokidar '../api/**/*.py' -c 'cd ../ && make openapi-json && pnpm run openapi-ts' --initial"
  },
  "dependencies": {
    "@mantine/hooks": "^8.3.8",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-form": "^1.25.0",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-router": "^1.136.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.554.0",
    "openapi-fetch": "^0.15.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "vercel": "^48.10.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/eslint-plugin-router": "^1.133.19",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router-devtools": "^1.136.11",
    "@tanstack/router-plugin": "^1.136.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "openapi-typescript": "^7.10.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2",
    "vitest": "^4.0.10",
    "web-vitals": "^5.1.0"
  }
}
